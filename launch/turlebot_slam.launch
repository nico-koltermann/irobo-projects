<?xml version="1.0"?>
<launch>

  <!-- <include file="$(find turtlebot3_gazebo)/launch/turtlebot3_world.launch"/> -->
  <include file="$(find turtlebot3_teleop)/launch/turtlebot3_teleop_key.launch"/>

  <!-- <include file="$(find turtlebot3_custom)/launch/turtlebot3_localization.launch"/> -->

  <!-- Parameter: http://wiki.ros.org/gmapping -->
  <include file="$(find turtlebot3_slam)/launch/turtlebot3_slam.launch">
    <param name="slam_methods" value="gmapping"/>
    <param name="map_update_interval" value="1.0"/>
    <param name="maxUrange" value="10.0"/>
    <param name="particles" value="30.0"/> 
    <param name="sigma" value="0.05"/> 
    <param name="lstep" value="0.05"/> 
    <param name="ogain" value="3.0"/> 
    <param name="astep" value="0.05"/> 
    <param name="iterations" value="5.0"/> 
    <param name="lsigma" value="0.075"/> 
  </include>

</launch>